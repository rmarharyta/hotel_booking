  "components": {
    "schemas": {
      "BookingRequest": {
        "required": [
          "CheckInDate",
          "CheckOutDate",
          "HotelName",
          "RoomId",
          "TotalPrice"
        ],
        "type": "object",
        "properties": {
          "RoomId": {
            "minLength": 1,
            "type": "string"
          },
          "HotelName": {
            "minLength": 1,
            "type": "string"
          },
          "CheckInDate": {
            "type": "string",
            "format": "date-time"
          },
          "CheckOutDate": {
            "type": "string",
            "format": "date-time"
          },
          "TotalPrice": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "HotelRequest": {
        "required": [
          "Address",
          "City",
          "Description",
          "Name"
        ],
        "type": "object",
        "properties": {
          "Name": {
            "minLength": 1,
            "type": "string"
          },
          "City": {
            "minLength": 1,
            "type": "string"
          },
          "Address": {
            "minLength": 1,
            "type": "string"
          },
          "Description": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RoomRequest": {
        "required": [
          "Capacity",
          "PricePerNight",
          "RoomNumber"
        ],
        "type": "object",
        "properties": {
          "RoomNumber": {
            "minLength": 1,
            "type": "string"
          },
          "Capacity": {
            "type": "integer",
            "format": "int32"
          },
          "PricePerNight": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "UserLoginRegister": {
        "required": [
          "Email",
          "Password"
        ],
        "type": "object",
        "properties": {
          "Email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "Password": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
